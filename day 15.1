class users:
    company_name = "CodeGalaxy"
    def __init__(self,name):
        self.name = name 
    def greet(self):
        print(f"Welcome {self.name} to {users.company_name}")
    @classmethod
    def change_name(cls,new_name):
        cls.company_name = new_name

user1 = users("Vishakha")
user1.greet()
user2 = users("Gagan")
user2.greet()
user3 = users("Sakshi")
print("Changing platform name...")
user3.company_name = "CodeVerse"
user3.greet()
user4 = users("Rashi")
user4.company_name = "CodeVerse"
user4.greet()

