num1=int(input("Enter the first number:"))
num2=int(input("Enter the second number:"))
operator=input("Enter any operator like +,-,*,%,/,//:\n")
if(operator=="+"):
    print("Addition for 2 numbers is:",num1+num2)
elif(operator == "-"):
    print("Subtraction for 2 numbers is:",num1-num2)
elif(operator == "*"):
    print("Multiplication for 2 numbers is:",num1*num2)
elif(operator == "%"):
    print("Remainder for 2 number is:",num1%num2)
elif(operator == "/"):
    print("Division for 2 number is:",num1/num2)
elif(operator =="//"):
    print("Floor division for 2 numbers is:",num1//num2)
else:
    print("Enter valid operator")
