class Exam:
    def __init__(self,score):
        self._score = score 

    @property
    def score(self):
        return self._score 
    
    @score.setter 
    def score(self,new):
        if 0 < new < 100:
            self._score = new 
        else:
            print("Invalid score")
    
    def grade(self):
        if self._score >= 90:
            print("A")
        elif self._score >= 75:
            print("B")
        elif self._score >= 50:
            print("C")
        else:
            print("Fail")

